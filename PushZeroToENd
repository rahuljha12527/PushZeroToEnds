
void PushZeroesEnd(int arr[], int n){
    
    
  int counter=0;
  for(int i=0;i<n;i++){
      if(arr[i]==0){
          counter++;
      }
  }  
    int j=0;
  for(int i=0;i<n;i++){
       if(arr[i]!=0){
          arr[j]=arr[i];
           j++;
      }
  }  
    
    for(int i=n-counter;i<n;i++){
        arr[i]=0;
    }
    
}
